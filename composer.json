{
  "name": "dotkernel/dot-controller-plugin-forms",
  "type": "library",
  "description": "DotKernel controller forms plugin for easy access to forms inside controllers",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "container-interop/container-interop": "^1.1",
    "dotkernel/dot-controller": "0.6.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",

    "dotkernel/dot-hydrator": "0.6.x-dev",
    "dotkernel/dot-filter": "0.6.x-dev",
    "dotkernel/dot-validator": "0.6.x-dev",
    "dotkernel/dot-inputfilter": "0.6.x-dev",
    "dotkernel/dot-form": "0.6.x-dev"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-form"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-inputfilter"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-filter"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-validator"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-hydrator"
    }
  ],
  "autoload": {
    "psr-4": {
      "Dot\\Controller\\Plugin\\Forms\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Controller\\Plugin\\Forms\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6-dev"
    }
  }
}
